    "autoprefixer-core": "^5.1.11",
    "babel-core": "^5.4.7",
    "babel-loader": "^5.1.3",
    "css-loader": "^0.14.3",
    "expose-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^0.8.1",
    "postcss-loader": "^0.4.3",
    "react-hot-loader": "^1.2.7",
