    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codedsignal/feei-webpacker.git"
  },
  "scripts": {
    "babel": "babel-node",
    "docs": "docco src/*.js src/**/*.js",
    "coverage": "babel-node -r node_modules/.bin/isparta cover node_modules/.bin/_mocha -- --recursive ./test",
    "test": "eslint ./src && mocha",
    "build": "npm test && gulp build && npm run docs",
    "git-commit": "git add -A . && git commit -a -m 'publish update'",
    "pr": "npm run build && npm run git-commit && npm version",
    "release-major": "npm run pr -- major && npm publish && git push --follow-tags",
    "release-patch": "npm run pr -- patch && npm publish && git push --follow-tags",
    "start": "mocha --watch --reporter dot"
  }
}
